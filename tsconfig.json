{
	"compilerOptions": {
		"target": "es2021",
		"lib": [
			"es2021"
		],
		"module": "es2022",
		"rootDir": "./src",
		"moduleResolution": "node",
		"types": [
			"@cloudflare/workers-types/2023-07-01"
		],
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}